<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.windows7.gichulgenerator">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_icon"
        android:supportsRtl="true"
        android:largeHeap="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RandomQuestionActivity"></activity>
        <activity android:name=".CalendarActivity"></activity>
        <activity android:name=".HistoryListActivity"></activity>
        <activity android:name=".CheckListActivity"></activity>
        <activity android:name=".RecheckQuestionActivity"></activity>
        <activity android:name=".SearchResultActivity"></activity>
        <activity android:name=".FreeboardActivity"></activity>
        <activity android:name=".ArticleActivity"></activity>
        <activity android:name=".ArticlePublishActivity"></activity>
        <activity android:name=".CommentActivity"></activity>
        <activity android:name=".QnaBoardActivity"></activity>
        <activity android:name=".ExamActivity"></activity>
        <activity android:name=".WebViewActivity"></activity>
        <activity android:name=".ExamResultListActivity"></activity>
        <service
            android:name=".FirebaseMessageService" android:enabled="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
    </application>
</manifest>